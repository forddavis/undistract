{
  "manifest_version": 2,
  "name": "Youtube ADD",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
        ],
        "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}